{
  "manifest_version": 3,
  "name": "Ruby 的橘猫起始页魔改",
  "version": "1.2.7",
  "description": "这是一个 Ruby 魔改的起始页",
  "homepage_url": "https://www.jvmao.net",
  "icons": {
    "16": "16x16.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "chrome_url_overrides": {
    "newtab": "src/newtab/index.html"
  },
  "host_permissions": [
    "*://*/*"
  ],
  "permissions": [
    "tabs",
    "favicon",
    "storage",
    "contextMenus"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "_favicon/*"
      ],
      "matches": [
        "<all_urls>"
      ],
      "extension_ids": [
        "*"
      ]
    },
    {
      "resources": [
        "src/content/index.js",
        "static/js/FavIconIcon-6844105d.js",
        "static/js/SearchIcon-1fd420e7.js",
        "static/js/content-1ab5d276.js",
        "static/js/styled-components.browser.esm-1bf82734.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "service_worker": "serviceWorker.js",
    "browser_action": {},
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/content/index.js"
      ],
      "css": [],
      "run_at": "document_end"
    }
  ]
}